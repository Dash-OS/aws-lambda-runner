{
  presets: [
    '@babel/preset-flow',
    [
      '@babel/preset-env',
      {
        useBuiltIns: 'usage',
        corejs: '3',
        include: [
          'es7.promise.finally',
          'es7.symbol.async-iterator',
          'es6.array.sort',
        ],
        targets: {
          node: '8.10',
        },
      },
    ],
  ],
  plugins: ['@babel/plugin-transform-runtime'],
}
