{
  "name": "aws-lambda-runner",
  "version": "2.5.3",
  "description": "Better handling of AWS Lambda Functions.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dash-OS/aws-lambda-runner/"
  },
  "scripts": {
    "postinstall": "npm run flow-typed",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "build:prod": "npm-run-all -s clean:dist -p build:prod:*",
    "clean:dist": "rimraf dist",
    "build:prod:transpile": "cross-env NODE_ENV=production babel -d dist lib",
    "build:prod:flow": "flow-copy-source lib dist",
    "prepublishOnly": "npm-run-all build:prod",
    "try": "babel-node ./test.js"
  },
  "keywords": [
    "aws",
    "lambda",
    "apex",
    "serverless",
    "api-gateway"
  ],
  "author": "Braden R. Napier <brady.napier@dashos.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dash-OS/aws-lambda-runner/issues"
  },
  "homepage": "https://github.com/Dash-OS/aws-lambda-runner",
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0-rc.2",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.2",
    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-flow": "^7.0.0-rc.2",
    "@babel/register": "^7.0.0-rc.2",
    "app-root-dir": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-promise": "^4.0.0",
    "flow-bin": "^0.79.1",
    "flow-copy-source": "^2.0.2",
    "flow-typed": "^2.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-rc.2",
    "qs": "^6.5.2"
  }
}
